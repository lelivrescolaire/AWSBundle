{
    "name": "lelivrescolaire/aws-bundle",
    "license": "MIT",
    "description": "Communicate with your AWS infrastructure from inside your Symfony 2 application",
    "autoload": {
        "psr-0": {
            "LLS\\Bundle\\AWSBundle": ""
        }
    },
    "target-dir": "LLS/Bundle/AWSBundle",
    "require": {
        "php": ">=5.3.0",
        "aws/aws-sdk-php": "~2.7"
    },
    "require-dev": {
        "atoum/atoum": "dev-master",
        "doctrine/common": "~2.4",
        "m6web/coke" : "~1.0",
        "m6web/Symfony2-coding-standard": "~1.1",
        "symfony/config": "~2.5",
        "symfony/dependency-injection": "~2.5",
        "symfony/http-kernel": "~2.5",
        "symfony/property-access": "~2.5",
        "symfony/yaml": "~2.5"
    },
    "suggests": {
        "lelivrescolaire/sqs-bundle": "Allows to ship messages into SQS queue"
    },
    "config": {
        "bin-dir": "bin"
    }
}
